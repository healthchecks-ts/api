[
  {
    "id": "api-service",
    "name": "API Service Health",
    "type": "http",
    "enabled": true,
    "interval": 30000,
    "timeout": 5000,
    "retries": 3,
    "tags": ["api", "critical"],
    "metadata": {
      "url": "https://api.example.com/health",
      "method": "GET",
      "expectedStatusCodes": [200],
      "headers": {
        "User-Agent": "HealthChecker/1.0"
      }
    }
  },
  {
    "id": "web-frontend",
    "name": "Web Frontend",
    "type": "http",
    "enabled": true,
    "interval": 45000,
    "timeout": 10000,
    "retries": 2,
    "tags": ["frontend", "web"],
    "metadata": {
      "url": "https://example.com",
      "method": "HEAD",
      "expectedStatusCodes": [200, 301, 302]
    }
  },
  {
    "id": "auth-service",
    "name": "Authentication Service",
    "type": "http",
    "enabled": true,
    "interval": 20000,
    "timeout": 3000,
    "retries": 2,
    "tags": ["auth", "critical"],
    "metadata": {
      "url": "https://auth.example.com/ping",
      "method": "GET",
      "expectedStatusCodes": [200],
      "expectedBody": "pong"
    }
  }
]